<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.u4bi.tipmoaproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tipmoa_logo"
        android:label="팁모아"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".InfoActivity">
        </activity>

        <activity
            android:name=".WebDialog"
            android:theme="@android:style/Theme.Dialog"
            android:windowSoftInputMode="stateHidden" android:configChanges="orientation|keyboardHidden">
        </activity>

        <activity
            android:name=".CalenDialog"
            android:theme="@android:style/Theme.Dialog"
            android:windowSoftInputMode="stateHidden" android:configChanges="orientation|keyboardHidden">
        </activity>
        <activity
            android:name=".SysDialog"
            android:theme="@android:style/Theme.Dialog"
            android:windowSoftInputMode="stateHidden" android:configChanges="orientation|keyboardHidden">
        </activity>

    </application>

</manifest>